Handler = () => {\n    dispatchEmail({ type: \"INPUT_BLUR\" })\n  };\n\n  const validatePasswordHandler = () => {\n    dispatchPassword({ type: \"INPUT_BLUR\" })\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    props.onLogin(emailState.value, passwordState.value);\n  };\n\n  return (\n    <Card className={classes.login}>\n      <form onSubmit={submitHandler}>\n\n        <Input\n          label=\"email\"\n          id=\"email\"\n          value={emailState}\n          onChange={emailChangeHandler}\n          onBlur={validateEmailHandler}\n        >\n        </Input>\n\n        <Input\n        label=\"password\"\n        id=\"password\"\n        value={passwordState}\n        onChange={passwordChangeHandler}\n        onBlur={validateEmailHandler}\n        >\n        </Input>\n\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","C:\\Users\\moham\\Desktop\\React\\MiniProject_3\\src\\components\\MainHeader\\Navigation.js",[],"C:\\Users\\moham\\Desktop\\React\\MiniProject_3\\src\\components\\UI\\Button\\Button.js",[],"C:\\Users\\moham\\Desktop\\React\\MiniProject_3\\src\\components\\UI\\Card\\Card.js",[],"C:\\Users\\moham\\Desktop\\React\\MiniProject_3\\src\\components\\store\\authContext.js",[],"C:\\Users\\moham\\Desktop\\React\\MiniProject_3\\src\\components\\UI\\Input\\Input.js",[],{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","severity":1,"message":"55","line":3,"column":10,"nodeType":"56","messageId":"57","endLine":3,"endColumn":20},{"ruleId":"54","severity":1,"message":"58","line":4,"column":8,"nodeType":"56","messageId":"57","endLine":4,"endColumn":19},{"ruleId":"54","severity":1,"message":"59","line":71,"column":9,"nodeType":"56","messageId":"57","endLine":71,"endColumn":32},"no-native-reassign",["60"],